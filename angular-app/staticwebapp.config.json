{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["*.{css,scss,js,png,gif,ico,jpg,svg}"]
  },
  "routes": [
		{
			"route": "/profile",
			"allowedRoles": [ "authenticated" ]
		},
		{
			"route": "/admin/*",
			"allowedRoles": [ "administrator" ]
		},
		{
			"route": "/images/*",
			"headers": {
				"cache-control": "must-revalidate, max-age=15770000"
			}
		},
		{
			"route": "/api/*",
			"methods": [ "GET" ],
			"allowedRoles": [ "registeredusers" ]
		},
		{
			"route": "/api/*",
			"methods": [ "PUT", "POST", "PATCH", "DELETE" ],
			"allowedRoles": [ "administrator" ]
		},
		{
			"route": "/api/*",
			"allowedRoles": [ "authenticated" ]
		}
	]
}
